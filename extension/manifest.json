{
  "name": "Chicken Out",
  "version": "0.1",
  "description" : "Blocks pictures of chickens",
  "permissions": ["activeTab", "<all_urls>"],
  "background": {
    "scripts": ["tf.js", "background.js"],
    "persistent": false
  },
	"content_scripts": [
		{
			"matches": [
				"http://*/*",
				"https://*/*"
			],
			"js": [
        "imagenet_classes.js",
				"contentscript.js"
			],
      "css": [
        "blur.css"
      ],
			"run_at": "document_start",
			"all_frames": true
		}
	],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
